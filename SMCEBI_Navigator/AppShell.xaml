<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="SMCEBI_Navigator.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:SMCEBI_Navigator.Views">
    <FlyoutItem Title="Picker Flyout shell">
        <ShellContent Title="PickerShellContent" ContentTemplate="{DataTemplate views:MapPickerPage}" />
    </FlyoutItem>
    <FlyoutItem Title="Map Flyout shell">
        <ShellContent ContentTemplate="{DataTemplate views:MapDisplayPage}" IsVisible="True" />
    </FlyoutItem>
</Shell>
